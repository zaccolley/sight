<section class="window window--pictures">
    <header>
        <h1>My Pictures</h1>
        <ul>
            <li id="min" title="Minimize">Min</li>
            <li id="max" title="Maximize">Max</li>
            <li id="close" title="Close">×</li>
        </ul>
    </header>

    <nav>
        <ul>
            <li><a href="http://instagram.com/zaccolley" target="_blank">Instagram</a></li>
        </ul>
    </nav>

    <section class="panel">
        <ul class="instagram-photos">
            {{# each instagram }}
            <li class="instagram-photo">
                <a href="{{ url }}">
                    <img src="{{ image }}" />
                    <span class="instagram-photo__caption">{{ caption }}</span>
                </a>
            </li>
            {{/ each }}
        </ul>
    </section>

</section><!-- ./windows--pictures -->

<div class="music">
<div class="music-inner">

    <div class="music__artist">{{ track.artist }}</div>
    <div class="music__song">{{ track.name }}</div>
    <img class="music__art" src="{{ track.image }}" />

    <div class="music__bar">
        <div class="music__bar__handle"></div>
    </div>

</div>
</div><!-- ./music -->

<div class="bar" title="Click here to begin">
    <div class="start-button"></div>
    <div class="clock">
        <div class="clock__time">{{ time }}</div>
    </div>
</div>

